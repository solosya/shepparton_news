                                {% if loop.index == 2  %}
                                    <div class="col-xs-12">
                                        <hr class="divide20-25 visible-xs-block">
                                    </div>
                                {% endif %}

                            {% endfor %}
                        </div>
                    </div>
                </div>
            </div>














