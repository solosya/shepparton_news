{{ set(this, 'title', _Blog.getBlogTitle()) }}
{% set photoVersion = '67' %}
{% set networkData  = _Network.getNetworkData() %}
{% set blogData     = _Blog.getBlog() %}

{% set config       = _Network.getThemeConfig() %}



{% set env = 'prod' %}

{% if config['env'] is defined %}
    {% set env = config['env'] %}
{% endif %}


{% set themeId = 'lVWIoKHyF5Dt' %}
{% if env == 'uat' %}
    {% set themeId = '4IAtb6AbAuFQDlF' %}
{% endif %}

{% set themeColor   = "#f03746" %}
{% if networkData['styling']['colours']['headingColor'] is defined %}
    {% set themeColor = networkData['styling']['colours']['headingColor'] %}
{% endif %}




<main id="photo-sales" role="main" data-env="{{env}}"></main>

<script>
    window.csrfToken    = "{{_AppHelper.getCsrfToken()}}";
    window.stripePublic = "{{networkData['stripe_publishable_key']}}";
    window.pageTitle    = "{{blogData['title']}}";
    window.searchid     = "{{blogData['id']}}";
    window.env          = "{{env}}";
    window.siteName     = "{{networkData['title']}}";
    window.themeColor   = "{{themeColor}}";
</script>


