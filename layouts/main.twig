{% endif %}

{% set paymeter = _AppHelper.getApplicationEnv() == 'PROD' ? true : false %}

{# {% set paymeter = true %} #}
{% set paymeterType  = config['piano']['paywall'] is defined and config['piano']['paywall'] == true ? 'piano' : 'syncronex' %}
