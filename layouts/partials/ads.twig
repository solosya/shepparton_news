                rubicontag.run(gptrun);
            });
            
            googletag.cmd.push(function() {
                var mapping1 = googletag.sizeMapping()
                                .addSize([1000, 200], [[970, 250], [970, 90], [728, 90]])
                                .addSize([768, 200], [[728, 90]])
                                .addSize([480, 200], [])
                                .addSize([360, 200], [])
                                .addSize([320, 400], [])
                                .build();

                var mapping2 = googletag.sizeMapping()
                                .addSize([1000, 200], [[300, 250]])
                                .addSize([768, 200], [[300, 250]])
                                .addSize([480, 200], ['fluid', [300, 250]])
                                .addSize([360, 200], ['fluid', [300, 250]])
                                .addSize([320, 400], ['fluid', [300, 250]])
                                .build();

                var mapping3 = googletag.sizeMapping()
                                .addSize([1000, 200], [[300, 600]])
                                .addSize([768, 200], [[300, 600]])
                                .addSize([480, 200], [[300, 250]])
                                .addSize([360, 200], [[300, 250]])
                                .addSize([320, 400], [[300, 250]])
                                .build();

                var mapping4 = googletag.sizeMapping()
                                .addSize([1000, 200], [[970, 250], [970, 90], [728, 90], [320, 50]])
                                .addSize([768, 200], [[728, 90]])
                                .addSize([480, 200], [[320, 50]])
                                .addSize([360, 200], [[320, 50]])
                                .addSize([320, 400], [[320, 50]])
                                .build();
                
                gptadslots[0] = googletag.defineSlot('/4082264/{{blogNetworkName}}', [[970, 250], [970, 90], [728, 90]], 'div-gpt-ad-leaderboard-1').setTargeting('POS', ['1']).defineSizeMapping(mapping1).setTargeting('BLOGPREFIX', ['{{blogPrefixName}}']).addService(googletag.pubads());
                gptadslots[1] = googletag.defineSlot('/4082264/{{blogNetworkName}}', [[970, 250], [970, 90], [728, 90], [320, 50]], 'div-gpt-ad-leaderboard-2').setTargeting('POS', ['2']).defineSizeMapping(mapping4).setTargeting('BLOGPREFIX', ['{{blogPrefixName}}']).addService(googletag.pubads());
                gptadslots[2] = googletag.defineSlot('/4082264/{{blogNetworkName}}', ['fluid', [300, 250]], 'div-gpt-ad-mrec-1').setTargeting('POS', ['1']).defineSizeMapping(mapping2).setTargeting('BLOGPREFIX', ['{{blogPrefixName}}']).addService(googletag.pubads());
                gptadslots[3] = googletag.defineSlot('/4082264/{{blogNetworkName}}', ['fluid', [300, 250]], 'div-gpt-ad-mrec-2').setTargeting('POS', ['2']).defineSizeMapping(mapping2).setTargeting('BLOGPREFIX', ['{{blogPrefixName}}']).addService(googletag.pubads());   
                gptadslots[4] = googletag.defineSlot('/4082264/{{blogNetworkName}}', ['fluid', [300, 250]], 'div-gpt-ad-mrec-3').setTargeting('POS', ['3']).defineSizeMapping(mapping2).setTargeting('BLOGPREFIX', ['{{blogPrefixName}}']).addService(googletag.pubads());
                gptadslots[5] = googletag.defineSlot('/4082264/{{blogNetworkName}}', ['fluid', [300, 250]], 'div-gpt-ad-mrec-4').setTargeting('POS', ['4']).defineSizeMapping(mapping2).setTargeting('BLOGPREFIX', ['{{blogPrefixName}}']).addService(googletag.pubads());
                gptadslots[6] = googletag.defineSlot('/4082264/{{blogNetworkName}}', [[300, 600], [300, 250]], 'div-gpt-ad-halfpage-ss').setTargeting('POS', ['1']).defineSizeMapping(mapping3).setTargeting('BLOGPREFIX', ['{{blogPrefixName}}']).addService(googletag.pubads());
                gptadslots[7] = googletag.defineSlot('/4082264/{{blogNetworkName}}', [[300, 600], [300, 250]], 'div-gpt-ad-halfpage-ss-2').setTargeting('POS', ['2']).defineSizeMapping(mapping3).setTargeting('BLOGPREFIX', ['{{blogPrefixName}}']).addService(googletag.pubads()); 
                gptadslots[8] = googletag.defineSlot('/4082264/{{blogNetworkName}}', [[1, 1]], 'div-gpt-ad-teads').setTargeting('BLOGPREFIX', ['{{blogPrefixName}}']).addService(googletag.pubads());
                gptadslots[9] = googletag.defineOutOfPageSlot('/4082264/{{blogNetworkName}}', 'div-gpt-ad-interstitial').setTargeting('BLOGPREFIX', ['{{blogPrefixName}}']).addService(googletag.pubads());

                
                for (var i=0; i < gptadslots.length; i++) {
                    rubicontag && rubicontag.setTargetingForGPTSlot && rubicontag.setTargetingForGPTSlot(gptadslots[i]);
                };
                
                googletag.pubads().enableSingleRequest();
                googletag.pubads().collapseEmptyDivs();
                googletag.pubads().setCentering(true);
                googletag.pubads().setTargeting('page-type', ['{{pageType}}']).setTargeting('tag', []);
                //googletag.pubads().enableSyncRendering();
                googletag.enableServices();
                
            });
            
            var gptran = false;
            var gptrun = function() {
                // don't run again if already ran
                if (gptran) {
                    return;
                }
                gptran = true;
                var gads = document.createElement('script');
                    gads.async = true;
                    gads.type = 'text/javascript';
                var useSSL = 'https:' === document.location.protocol;
                    gads.src = (useSSL ? 'https:' : 'http:') + '//www.googletagservices.com/tag/js/gpt.js';
                var node = document.getElementsByTagName('script')[0];
                    node.parentNode.insertBefore(gads, node);
            };

            var nextSlotId = 1;
            function generateNextSlotName() {
               var id = nextSlotId++;
               return 'adslot' + id;
            }

             
            function loadNextAd() {
               var adSlot = document.getElementById("newAdSlot");
               var slotName = generateNextSlotName();
               var slotDiv = document.createElement('div');
               adSlot.appendChild(slotDiv);
               slotDiv.id = slotName;
               slotDiv.setAttribute( 'class', 'google_ad google_ad_leaderboard' );
               adSlot.id = "";
               adSlot.setAttribute( 'class', 'ad-container' );
               
               googletag.cmd.push(function() {
                 var slot = googletag.defineSlot('/4082264/{{blogNetworkName}}', [[970, 250], [970, 90], [728, 90], [300, 250]], slotName).setTargeting('POS', ['1']).defineSizeMapping(mapping).setTargeting('BLOGPREFIX', ['{{blogPrefixName}}']).addService(googletag.pubads());
                 googletag.display(slotName);
                 googletag.pubads().refresh([slot]);
               });
            }
</script>

