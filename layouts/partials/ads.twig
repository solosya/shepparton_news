{% elseif (blogData.title | lower) == 'australian rules' or (blogData.title | lower) == 'cricket' or (blogData.title | lower) == 'golf' or (blogData.title | lower) == 'netball' or (blogData.title | lower) == 'racing' or (blogData.title | lower) == 'soccer' or (blogData.title | lower) == 'tennis' %}
